<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="514.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.YourOrderItemController">
   <children>
      <Label fx:id="numberLabel" prefHeight="18.0" prefWidth="26.0" text="#" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="idLabel" layoutX="10.0" layoutY="28.0" prefHeight="18.0" prefWidth="26.0" text="ID" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="totalLable" layoutX="36.0" layoutY="28.0" prefHeight="18.0" prefWidth="72.0" text="Total" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="statusLabel" layoutX="63.0" layoutY="28.0" prefHeight="18.0" prefWidth="106.0" text="Status" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="addressLabel" layoutX="135.0" layoutY="28.0" prefHeight="18.0" prefWidth="170.0" text="Address" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="114.0">
         <children>
            <Button fx:id="detailButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="70.0" style="-fx-background-color: red;" text="Detail" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</HBox>
